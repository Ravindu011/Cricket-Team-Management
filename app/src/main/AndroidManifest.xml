<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ccc"
        android:label="Team Management System"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cricketapp"
        tools:targetApi="31">
        <activity
            android:name=".ViewPlayerProfile"
            android:exported="false" />
        <activity
            android:name=".ViewPlayers"
            android:exported="false" />
        <activity
            android:name=".LoginToAdmin"
            android:exported="false" />
        <activity
            android:name=".Updateplayer"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PlayerListBar"
            android:exported="false" />
        <activity
            android:name=".Add_Player"
            android:exported="false" />
        <activity
            android:name=".Admin_Home"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>